package Exampleweek1;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.firefox.FirefoxDriver;

public class TRY5CSSLOCATOR1
{
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		WebDriver driver = new FirefoxDriver();
		driver.get("http://www.google.com/");
		driver.findElement(By.xpath("//a[contains(@id,'b_7')]")).click();
	//	driver.findElement(By.cssSelector("input[id='Email']")).sendKeys("Send a value");
	//	driver.findElement(By.cssSelector("input[id='Email'][name='Email']")).sendKeys("Send a value");
		driver.findElement(By.cssSelector("#Email")).sendKeys("Send a value");
		
		
	}
}
